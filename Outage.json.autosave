."," * 11/2/2023"," */","","/**","    ██████  ██       █████  ███████ ███████ ███████ ███████ ","    ██      ██      ██   ██ ██      ██      ██      ██      ","    ██      ██      ███████ ███████ ███████ █████   ███████ ","    ██      ██      ██   ██      ██      ██ ██           ██ ","    ██████  ███████ ██   ██ ███████ ███████ ███████ ███████"," */","","class Point {","    constructor(x = 0, y = 0) {","        Object.assign(this, {x, y});","    }","","    get coords() {","        return [this.x, this.y]","    }","","    static distBetween(p1, p2) {","        return Math.sqrt(Math.pow(p2.x - p1.x, 2) + Math.pow(p2.y - p1.y, 2))","    }","","    static calcDirectionVecAB(a, b) {","        return new Vector(","            b.x - a.x,","            b.y - a.y","        ).normalize();","    }","","    static sub(p1, p2) {","        return new Vector(p1.x - p2.x, p1.y - p2.y);","    }","}","","/**"," * "," * A simple vector class with an x and a y component which default to 0"," * "," * All vector operations work functionally, creating a new vector instead of modifying the current one"," * "," * Implements:"," *  - Add"," *  - Sub"," *  - Normalize"," * "," */","class Vector {","    constructor(x = 0, y = 0) {","        Object.assign(this, { x, y })","    }","","    // may not work properly if used on a non-unit vector","    get angle() {","        return Math.atan2(this.y, this.x);","    }","","    get coords() {","        return [this.x, this.y]","    }","","    get length() {","        return Math.sqrt(this.x * this.x + this.y * this.y)","    }","","    normalize() {","        let length = this.length;","        return new Vector(this.x / length, this.y / length);","    }","","    static mult_by_const(v1, c) {","        return new Vector(v1.x * c, v1.y * c);","    }","","    static add(v1, v2) {","        return new Vector(v1.x + v2.x, v1.y + v2.y);","    }","","    static sub(v1, v2) {","        return new Vector(v1.x - v2.x, v1.y - v2.y);","    }","}","","/**"," * A simple ray class. Has a point and a direction."," */","class Ray {","    /**","     * @param {Point} pos","     * @param {Vector} dir","     */","    constructor(pos, dir) {","        this.pos = pos;","        this.dir = dir;","    }","","    get angle() {","        return Math.atan2(this.dir.y, this.dir.x);","    }","","    calcInterceptWithXConstant(x) {","        let t = (x - this.pos.x) / this.dir.x","","        if (t < 0) { return false }","","        return new Point(this.pos.x + this.dir.x * t, this.pos.y + this.dir.y * t);","    }","","    calcInterceptWithYConstant(y) {","        let t = (y - this.pos.y) / this.dir.y","","        if (t < 0) { return false }","","        return new Point(this.pos.x + this.dir.x * t, this.pos.y + this.dir.y * t);","    }","","    static fromAngle(pos, angle) {","        return new Ray(","            pos,","            new Vector(","                Math.cos(angle),","                Math.sin(angle)","            )","        )","    }","","    static fromPoints(pos, pos2) {","        return new Ray(","            pos,","            Point.calcDirectionVecAB(","                pos,","                pos2","            )","        )","    }","}","","/**","     ██████  ██████  ███    ██ ███████ ████████  █████  ███    ██ ████████ ███████ ","    ██      ██    ██ ████   ██ ██         ██    ██   ██ ████   ██    ██    ██      ","    ██      ██    ██ ██ ██  ██ ███████    ██    ███████ ██ ██  ██    ██    ███████ ","    ██      ██    ██ ██  ██ ██      ██    ██    ██   ██ ██  ██ ██    ██         ██ ","     █████   ██████  ██   ████ ███████    ██    ██   ██ ██   ████    ██    ███████                                  "," */","","const VIEWPORT_WIDTH = runtimeScene.getViewportWidth();","const VIEWPORT_HEIGHT = runtimeScene.getViewportHeight();","","const HALF_VIEWPORT_WIDTH = VIEWPORT_WIDTH / 2;","const HALF_VIEWPORT_HEIGHT = VIEWPORT_HEIGHT / 2;","","const VIEWPORT_CENTER = new Point(","    HALF_VIEWPORT_WIDTH,","    HALF_VIEWPORT_HEIGHT",")","","const VIEWPORT_EDGE = Object.freeze({","    Left: Symbol(\"Left Edge\"),","    Right: Symbol(\"Right Edge\"),","    Top: Symbol(\"Top Edge\"),","    Bottom: Symbol(\"Bottom Edge\")","})","","// Starts from the top left corner, and goes clockwise","const CORNERS = [","    new Point(0, 0),","    new Point(VIEWPORT_WIDTH, 0),","    new Point(VIEWPORT_WIDTH, VIEWPORT_HEIGHT),","    new Point(0, VIEWPORT_HEIGHT)","]","","const CAMERA_INSTANCE = runtimeScene.getScene().getLayer(\"\");","const CAMERA_POSITION = new Point(","    CAMERA_INSTANCE.getCameraX(),","    CAMERA_INSTANCE.getCameraY()",")","","const SHAPE_DRAWER = runtimeScene.getObjects(\"FlashlightShapePainter\")[0];","","const INTERACTABLE_OBJECTS = [","    \"LightWall\",\"Tree\",","]","","// Edges will correspond to their left corner","function edgeToCornerIndex(edge) {","    switch(edge) {","        case VIEWPORT_EDGE.Top:","            return 0;","        case VIEWPORT_EDGE.Right:","            return 1;","        case VIEWPORT_EDGE.Bottom:","            return 2;","        case VIEWPORT_EDGE.Left:","            return 3;","    }","}","","/**"," * "," * Takes two screen edges and returns the corners that aren't contained within two points that lie on those edges"," * "," * @param {VIEWPORT_EDGE} edge1 - the first edge"," * @param {VIEWPORT_EDGE} edge2 - the second edge"," * "," * @return [Point] - An array of Points corresponding to the corners"," * "," */","function getExteriorCorners(edge1, edge2, vertices = 4) {","    let current_corner = (edgeToCornerIndex(edge2) + 1) % vertices;","    let end_corner = edgeToCornerIndex(edge1);","    let corners = [];","","    while (current_corner !== end_corner) {","        corners.push(CORNERS[current_corner]);","        current_corner = (current_corner + 1) % vertices;","    }","","    corners.push(CORNERS[end_corner]);","    return corners;","}","","/**"," * "," * Takes two screen edges and returns the corners that are contained within two points that lie on those edges"," * "," * @param {VIEWPORT_EDGE} edge1 - the first edge"," * @param {VIEWPORT_EDGE} edge2 - the second edge"," * "," * @return [Point] - An array of Points corresponding to the corners"," * "," */","function getInteriorCorners(edge1, edge2) {","    let current_corner = (edgeToCornerIndex(edge1) + 1) % 4;","    let end_corner = (edgeToCornerIndex(edge2) + 1) % 4;","    let corners = [];","","    while (current_corner !== end_corner) {","        corners.push(CORNERS[current_corner]);","        current_corner = (current_corner + 1) % 4;","    }","","    return corners;","}","","function getExteriorCornersFromIndex(corner1, corner2, vertices = 4) {","    let corners = [];","","    while (corner1 !== corner2) {","        corner1 = corner1 - 1;","","        if (corner1 < 0) {","            corner1 = vertices - 1;","        }","","        corners.push(corner1);","    }","","    return corners;","}","","function drawShadowCast(...points) {","    SHAPE_DRAWER.beginFillPath(points[0], points[1]);","    ","    for (let i = 0; i < points.length; i += 2) {","        SHAPE_DRAWER.drawPathLineTo(points[i], points[i + 1]);","    }","","    SHAPE_DRAWER.endFillPath();","}","","function drawDarkener(){","    SHAPE_DRAWER.beginFillPath(0, 0);","    for (let i = 0; i < CORNERS.length; i += 1) {","        SHAPE_DRAWER.drawPathLineTo(CORNERS[i].x, CORNERS[i].y);","    }","","    SHAPE_DRAWER.endFillPath();","}","","/**"," * "," * The input ray's position should be converted to screen space."," * "," * @param {Ray} The casting ray"," * @return [Point, VIEWPORT_EDGE]"," * "," */","function getRayViewportIntercept(ray) {","    let intercepts = [","        [ray.calcInterceptWithXConstant(0), VIEWPORT_EDGE.Left],","        [ray.calcInterceptWithXConstant(VIEWPORT_WIDTH), VIEWPORT_EDGE.Right],","        [ray.calcInterceptWithYConstant(0), VIEWPORT_EDGE.Top],","        [ray.calcInterceptWithYConstant(VIEWPORT_HEIGHT), VIEWPORT_EDGE.Bottom]","    ].filter((pair) => pair[0] !== false);","    ","    return Point.distBetween(ray.pos, intercepts[0][0]) < Point.distBetween(ray.pos, intercepts[1][0]) ? intercepts[0] : intercepts[1];","}","","/**"," * @param {Point} point"," */","function toScreenSpace(point) {","    return new Point(","        point.x - CAMERA_POSITION.x + HALF_VIEWPORT_WIDTH,","        point.y - CAMERA_POSITION.y + HALF_VIEWPORT_HEIGHT","    )","}","","// ChatGPT v.4","function degreesToRadians(angleDegrees) {","  let angleRadians = angleDegrees * (Math.PI / 180);","","  angleRadians = angleRadians % (2 * Math.PI);","","  if (angleRadians > Math.PI) {","    angleRadians -= 2 * Math.PI;","  } else if (angleRadians <= -Math.PI) {","    angleRadians += 2 * Math.PI;","  }","","  return angleRadians;","}","","function drawRay(ray, length = 100, thickness = 5) {","    SHAPE_DRAWER.drawLine(...ray.pos.coords, ...Vector.add(ray.pos, Vector.mult_by_const(ray.dir, length)).coords, thickness);","}","","function drawPoint(point, dimensions) {","    SHAPE_DRAWER.drawEllipse(...point.coords, dimensions, dimensions);","}","","function normalizeAngle(angle) {","    if (angle < 0) {","        return Math.PI * 2 + angle;","    }","","    return angle;","}","","function differenceBetweenAngles(centerAngle, targetAngle) {","    let difference = normalizeAngle(targetAngle) - normalizeAngle(centerAngle);","","    if (difference < -Math.PI) {","        difference += Math.PI * 2;","    }","","    if (difference > Math.PI) {","        difference -= Math.PI * 2;","    }","","    return difference;","}","","function pointIsInScreen(point) {","    return point.x >= 0 && point.y >= 0 && point.x <= VIEWPORT_WIDTH && point.y <= VIEWPORT_HEIGHT;","}","","function boundingBoxIsInScreen(box) {","    box.forEach((point) => {","        if (!pointIsInScreen(new Point(","            point.x,","            point.y","        ))) {","            return false;","        }","    })","","    return true;","}","","/**","    ██████  ██████   ██████   ██████  ██████   █████  ███    ███ ██ ","    ██   ██ ██   ██ ██    ██ ██       ██   ██ ██   ██ ████  ████ ██ ","    ██████  ██████  ██    ██ ██   ███ ██████  ███████ ██ ████ ██ ██ ","    ██      ██   ██ ██    ██ ██    ██ ██   ██ ██   ██ ██  ██  ██    ","    ██      ██   ██  ██████   ██████  ██   ██ ██   ██ ██      ██ ██ "," */","","let player_instance = objects[0];","let FOV = player_instance.getVariables().get(\"FOV\").getAsNumber();","let fov_range = degreesToRadians(FOV / 2);","","let player_position = new Point(","    player_instance.getX() + player_instance.getCenterX(),","    player_instance.getY() + player_instance.getCenterY()",")","","let input_manager = runtimeScene.getGame().getInputManager()","let mouse_position = new Point(","    input_manager.getMouseX(),","    input_manager.getMouseY()",")","","let player_screen_position = toScreenSpace(player_position);","let player_mouse_ray = Ray.fromPoints(player_screen_position, mouse_position);","","let low_fov_bound = Ray.fromAngle(player_screen_position, player_mouse_ray.angle - fov_range);","let high_fov_bound = Ray.fromAngle(player_screen_position, player_mouse_ray.angle + fov_range);","","let [low_bound_intercept, lbi_edge] = getRayViewportIntercept(low_fov_bound);","let [high_bound_intercept, hbi_edge] = getRayViewportIntercept(high_fov_bound);","","let exterior_corners = getExteriorCorners(lbi_edge, hbi_edge);","","SHAPE_DRAWER.setFillOpacity(155);","drawDarkener();","","SHAPE_DRAWER.setFillOpacity(220);","drawShadowCast(","    ...player_screen_position.coords,","    ...high_bound_intercept.coords,","    ...exterior_corners.reduce((points, corner) => points.concat(corner.coords), []),","    ...low_bound_intercept.coords,",");","","SHAPE_DRAWER.setFillOpacity(255);","INTERACTABLE_OBJECTS.forEach((object_type) => {","    let instances = runtimeScene.getObjects(object_type);","","    instances.forEach((instance) => {","        let center = toScreenSpace(new Point(instance.getCenterXInScene(), instance.getCenterYInScene()));","        let center_angle = Point.calcDirectionVecAB(player_screen_position, center).angle;","        let hitbox = instance.getHitBoxes()[0].vertices;","        let corners = hitbox.map((corner) => toScreenSpace(new Point(corner[0], corner[1])));","","        if (!boundingBoxIsInScreen(corners)) {","            return;","        }","","        const vertex_count = corners.length;","        ","        let rays = corners.map((corner) => {","            return new Ray(player_screen_position, Point.calcDirectionVecAB(player_screen_position, corner));","        });","        ","        let angles = rays.map((ray) => differenceBetweenAngles(center_angle, ray.angle));","","        let low_index = angles.indexOf(Math.min(...angles));","        let high_index = angles.indexOf(Math.max(...angles))","","        let low_ray = rays[low_index];","        let [low_ray_intercept, lri_edge] = getRayViewportIntercept(low_ray);","","        let high_ray = rays[high_index];","        let [high_ray_intercept, hri_edge] = getRayViewportIntercept(high_ray);","","        let contained_corners = getInteriorCorners(lri_edge, hri_edge, vertex_count);","","        let exterior_corners = getExteriorCornersFromIndex(low_index, high_index, vertex_count).map((index) => corners[index]);","","        drawShadowCast(...corners[low_index].coords,","                        ...exterior_corners.reduce((points, corner) => points.concat(corner.coords), []),","                        ...corners[high_index].coords, ","                        ...high_ray_intercept.coords, ","                        ...contained_corners.reverse().reduce((points, corner) => points.concat(corner.coords), []), ","                        ...low_ray_intercept.coords);","    })","})"],"parameterObjects":"Player","useStrict":true,"eventsSheetExpanded":true}],"parameters":[]}],"layers":[{"ambientLightColorB":200,"ambientLightColorG":200,"ambientLightColorR":200,"camera3DFarPlaneDistance":10000.0,"camera3DFieldOfView":45.0,"camera3DNearPlaneDistan